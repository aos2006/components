@import './css/mixins.styl';

.listingWrapper {
  clearfix();
  width: 100%;
}

.traderPanelsContainer {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: stretch;
  align-self: flex-start;
  justify-content: space-between;
  padding: 3px;

  &.mobileTraderPanelsContainer
    padding: 0;
    height: 100%;

    .buySellPanel {
      //border: none;
      margin-bottom: 8px;
    }
  }

  @media screen and (max-width: 1280px) {
    flex-wrap: wrap;

    .leftPanel {
      min-height: 0 !important;
    }

    .rightPanel {
      min-width: 0;
      max-width: 100%;
      width: 100%;
      flex-flow: row wrap;
      justify-content: space-between;

      .emptyBlock {
        min-height: 100px;
        height: 100px;
      }

      > div {
        &:nth-child(1) {
          flex-grow: 1;
          flex-basis: 300px;
          width: auto;
          margin-right: 2px

        }

        &:nth-child(2) {
          flex-grow: 1;
          flex-basis: 300px;
          width: auto;
          margin-left: 2px;
        }

        &:nth-child(3) {
          flex-basis: 100%;
        }
      }
    }
  }

  @media screen and (max-width: 1000px) {
    flex-flow: column nowrap;

    .leftPanel, .rightPanel, .centerPanel {
      width: auto !important;
      flex-basis: auto;
      min-width: auto;
      margin-left: 0;
      margin-right: 0;
    }

    .centerPanel {
      order: 0;
    }
  }
}

.leftPanel {
  tradingBlock();
  margin: 2px;
  width: 355px !important;
  padding: 0 !important;
}

.centerPanel {
  display: flex;
  flex-flow: column nowrap;
  min-width: 400px;
  flex-basis: 0;
  flex-grow: 1;
  margin: 0 2px;

  .chartBlock {
    tradingBlock();
    padding-bottom: 10px;
  }

  .cryptoInfoBlock {
    tradingBlock();
    antTabs();
    flex: 1;

    &.mobileCryptoInfoBlock {
      margin: 0;
      border: 0;
      background: none;

      .icoTopBlock {
        margin: 0;
        border: none;
      }

      .buySellPanel {
        border: 0;

        .leftColumn, .rightColumn {
          width: 41%;
        }
      }

      :global(.lat-tabs-nav) {
        height: 47px;
      }
      :global(.lat-tabs-tab) {
        padding: 13px 2px;
        margin: 0 12px 0 0 !important;

        &:last-child {
          margin-right: 0 !important;
        }

        &:first-child {
          margin-left: 0 !important;
        }
      }
    }

    &.isCrypto {
    }

    .videoContainer {
      height: 300px;
    }

    :global(.lat-tabs-tab) {
      margin: 0 20px 0 0 !important;

      &:last-child {
        margin-right: 0 !important;
      }
    }

    :global(.lat-tabs-bar) {
      border-bottom: 1px solid #e2e2e2;
    }

    .description {
      padding: 10px;
      background-color: white;
      overflow: auto
      smallScroll()

      .line {
        width: 100%;
        line-height: 20px;
        display: flex;
        flex-flow: row nowrap;

        .key {
          color: $grey3;
          margin-right: 10px;
          white-space: nowrap;
        }

        .value {

        }

        p {
          word-break: break-word;
        }
      }

      .icoLinkWrapper {
        margin-bottom: 5px;
        .icoLink {
          text-transform: capitalize;
          font-size: 18px;

          i {
            width: 19px;
            text-align: center;
            margin-right: 2px;
          }
        }
      }
      .newsBlock {
        line-height: 20px;

        .newsTitle {
          display: block;
          font-weight: bold;

          .timeAgo {
            display: inline-block;
            margin-left: 10px;
            color: $grey3;
            font-weight: 500;
          }
        }

        .newsText {
          display: block;
          margin-bottom: 15px;

          .readMore {
            display: inline-block;
            margin-left: 10px;
          }
        }
      }
    }
    .twitterTab {
      > div {
        background: #fff;
        smallScroll()
      }
    }

    .teamMemberList {
      display: flex;
      flex-direction: row;
      flex-wrap: wrap;
      flex-grow: 0;
      flex-shrink: 0;
      padding-top: 30px;
    }

    .teamMember {
      display: flex;
      flex-direction: row;
      align-content: flex-start;
      justify-content: space-between;
      margin-bottom: 15px;

      .teamMemberPhoto {
        width: 60px;
        height: 60px;

        .teamMemberPhotoImage {
          width: 60px;
          height: 60px;
          background-size: cover;
          border-radius: 60px;
        }
      }

      .teamMemberDescription {
        margin-left: 10px;
        width: 150px;
      }
      .teamMemberName {
        color: #151524;
        font-size: 18px;
        line-height: 20px;
      }
      .teamMemberPosition {
        font-size: 14px;
        color: #666666;
        line-height: 20px;
      }
      .teamMemberLinkedin {
        font-size: 14px;
        line-height: 20px;
        text-decoration: none;
        color: white;
        :hover {
          text-decoration: none;
          color: white;
        }
      }
      .teamMemberIcon {
        font-size: 10px;
        color: white;
        background-color: #0096ac;
        padding: 2px 2px;
        border-radius: 3px;
      }

    }
  }
}

.select {
  margin-right: 4px;
  width: 80px;
  font-weight: 500;

  :global(.ant-select-selection) {
    height: 28px;
    border-color: #c9cbcd;
    box-shadow: none !important;

    :global(.ant-select-selection__rendered) {
      line-height: 27px;
      font-size: 11px;
    }

    :global(.ant-select-selection-selected-value) {
      color: $grey3;
      transition: color .2s;
    }

    :global(.ant-select-arrow) {
      transition: color .2s;
      border: 4px solid transparent;
      border-top-color: $grey3;
      margin-top: 3px;

      &:before {
        display: none;
      }
    }
  }

  &:global(.ant-select-enabled) {
    :global(.ant-select-selection) {
      &:hover {
        color: $blue1;
        background: alpha($blue1, 0.3);
        border-color: transparent;

        :global(.ant-select-selection-selected-value) {
          color: $blue1;
        }

        :global(.ant-select-arrow) {
          border-top-color: $blue1;
        }
      }
    }
  }

  &.selectActive {
    :global(.ant-select-selection) {
      background: $blue1 !important;
      border-color: $blue1 !important;

      :global(.ant-select-selection-selected-value) {
        color: #fff !important;
      }

      :global(.ant-select-arrow) {
        border-top-color: #fff !important;
      }
    }
  }
}

.rightPanel {
  width: 450px;
  min-width: 450px;
  max-width: 450px;
  display: flex;
  flex-flow: column nowrap;
  margin: 0 2px;
}

.icoTopBlock {
  tradingBlock();

  .header {
    border-bottom: 1px solid #e2e2e2;
    line-height: 37px;
    min-height: 38px;
    padding: 0 10px;
    font-size: 14px;
    font-weight: bold;
    text-transform: uppercase;
  }

  .info {
    padding: 10px;
    font-weight: 500;
  }

  .icoName {
    font-size: 20px;
    padding-bottom: 10px;
  }

  .greyText {
    font-size: 14px;
    color: $grey3;
    line-height: 21px;
  }

  .textPadded {
    padding: 10px 0;
  }

  .progressBlock {
    padding: 20px 0;

    .progressBlockTitle {
      font-size: 14px;
      margin-bottom: 5px;
    }

    :global(.ant-progress-bg) {
      background: $blue1;
      height: 6px !important;
    }

    :global(.ant-progress) {
      height: 6px;
      display: block;

      div {
        vertical-align: top;
        height: 6px;
      }
    }

    .progressBlockBottom {
      margin-top: 5px;
    }
  }
}

.emptyBlock {
  tradingBlock();
  min-height: 826px;
  height: 100%;
}

// Animations
.enter {
  opacity: 0;
  transition: opacity .4s;
}

.enterActive {
  opacity: 1;
}

.exit {
  opacity: 1;
  transition: opacity .2s;
}

.exitActive {
  opacity: 0;
}

.noSelect * {
  user-select: none !important;
  cursor: row-resize !important;
}

.selectDropdown {
  :global(.ant-select-dropdown-menu-item) {
    font-size: 11px;
  }

  :global(.ant-select-dropdown-menu-item-disabled) {
    display: none;
  }
}

.buySellPanel {
  tradingBlock();
  padding: 10px;
  min-width: 280px;

  .row {
    clearfix();
    width: 100%;
    padding-bottom: 8px;

    &.mobileRow {
      .caption {
        width: 20%;
      }
      .leftColumn {
        width: 80%;
        padding: 0;
      }
      .mobileTotalRow {
        padding: 0;
        width: 100%;
        float: left;
        .valueBlock {
          padding-left: 0;
          font-weight: 400;
        }
      }
    }

    .mobileBalanceWrap {
      width 82%;
      padding-left: 10px;
      float: left;
      .leftColumn, .rightColumn {
        float: none;
        width: 100%;
        text-align: left;
        padding: 0;

        .valueBlock {
          text-align: left;
          padding: 0;
          line-height: 1;
        }
      }
    }

    &.priceRow {
      .leftColumn {
        width: 50%;
      }
      .rightColumn {
        width: 32%;
      }
    }

    &:last-child {
      padding-bottom: 0;
    }
  }

  .caption {
    float: left;
    width: 18%;
    text-transform: uppercase;
    font-size: 11px;
    padding-top: 2px;
    line-height: 26px;
    color: $grey3;
    user-select: none;
  }

  .valueBlock {
    font-size: 12px;
    padding-top: 2px;
    line-height: 26px;
    font-weight: 500;
  }

  .leftColumn, .rightColumn {
    position relative;
    float: left;
    width: 41%;
    padding-right: 10px;
  }

  .rightColumn {
    float: right;
    padding-right: 0;
    padding-left: 10px;
    .valueBlock {
      text-align: right;
      padding-left: 10px;

      .caption {
        padding-top: 0;
        width: auto;
      }
    }

    .totalValue {
      float: right;
    }
  }

  .currency {
    display: inline-block;
    width: 36px;
    font-size: 11px;
  }

  .smallHint {
    position: absolute;
    z-index: 1;
    right: 0;
    top: 22px;
    text-align: right;
    font-size: 11px;
    color: $grey3;
    line-height: 10px;
  }

  .tradingDisabled {
    float: left;
    width: 60%;
    line-height: 28px;
    font-weight: 500;
    font-size: 12px;
  }

  .select {
    width: 100%;

    :global(.ant-select-selection) {
      border-color: $grey2;
      border-radius: 3px;
    }
  }

}

.mobileBuySellPanel {
  height: 44px;
  background-color transparent;
  border: none;
  flex: 0 0 44px;
  padding: 10px 0 0;
  min-width: auto;

  .leftColumn, .rightColumn {
    float: left;
    width: 48%;
    height: 100%;

    div {
      height: 100%;
      line-height: 36px !important;
    }
  }

  .rightColumn {
    float: right;
  }
}

.buySellButtons {
  position relative;

  &.mobileBuySellButtons {
    .caption {
      width: 22%;
    }

    .leftColumn, .rightColumn {
      width: 39%;
    }
  }

  .buySellButton {
    position: relative;
    border-radius: 3px;
    color: white;
    text-transform: uppercase;
    line-height: 28px;
    font-weight: 600;
    text-align: center;
    cursor: pointer;
    width: 100%;
    transition: background .2s;
    user-select: none;
    outline: none !important;

    &:after {
      content: '';
      display: block;
      position: absolute;
      z-index: 1;
      top: 50%;
      margin-top: -8px;
      left: 10px;
      border: 2px solid rgba(255, 255, 255, 0.6);
      border-bottom-color: transparent;
      border-radius: 50%;
      width: 16px;
      height: 16px;
      animation: spin 1s linear infinite;
      opacity: 0;
      transition: opacity 2s;
    }

    &.loading:after {
      opacity: 1;
      transition: opacity 0.2s;
    }

    &.buy {
      background-color: #3dbd7d;

      &:hover {
        background-color: lighten(#3dbd7d, 10%);
      }

      &:focus {
        box-shadow: 0 0 5px #3dbd7d;
      }
    }

    &.sell {
      background-color: #e36776;

      &:hover {
        background-color: lighten(#e36776, 10%);
      }

      &:focus {
        box-shadow: 0 0 5px #e36776;
      }
    }

    &.disabled {
      background-color: $grey3 !important;
      cursor: no-drop;
      box-shadow: none !important;
    }
  }
}

.ordersContainer {
  tradingBlock();
  antTabs();
  //height: 240px;

  .disabledRow {
    opacity: 0.4;
  }

  :global(.lat-tabs-tab) {
    white-space: nowrap !important;
  }

  :global(.lat-tabs-nav) {
    justify-content: space-between;
    display: flex;
    flex-flow: row nowrap;
  }

  .orderTabs {
    @include sm-less() {
    width: 100% !important;
    overflow: hidden;
  }
}

.greenContent {
  color: $green1;
}

.redContent {
  color: $red1;
}

.infoPreview {
  margin-left: 10px;
  margin-top: 1px;
  width: 17px;
  height: 17px;
  border-radius: 40px;
  border: solid 1px $blue1;
  color: $blue1;
  font-size: 11px;
  font-weight: 500;
  line-height: 17px;
