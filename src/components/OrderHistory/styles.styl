  display: inline-block;
  vertical-align: middle
  font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
}
.infoPreviewWrap{
  white-space: nowrap;
}

.cancelButton {
  cursor: pointer;
  font-size: 14px;
  color: #9b9b9b;
  vertical-align: middle;

  &:hover {
    color: $blue1;
  }
}

.myOrders {
  .timeColumn {
    width: 65px;
  }

  .priceColumn {
    width: 58px;
    text-align: right;
  }

  .centerColumn {
    width: 65px;
    text-align: right;
  }

  .sideColumn {
    width: 30px;
    text-align: right;
  }

  .actionColumn {
    width: 30px;
    padding-left: 6px;
    text-align: right;
  }
}

.marketHistory {
  td, th {
    text-align: right !important;

    &:first-child {
      text-align: left !important;
    }
  }
}

.ordersTable {
  :global(.lat-table) {
    border-left: 0;
    border-right: 0;
  }

  :global([class*='table-wrapper']) {
    smallScroll()
  }


  :global(.lat-table-placeholder) {
    height: (22 * 8)px;
    line-height: (22 * 8)px;
    padding: 0 16px;
    border: 0;
  }

  table {
    border-spacing: 0;

    thead {
      overflow-x: hidden;
      tr {
        th {
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
          padding: 0;
          height: 25px;

          span {
            display: block;
          }
          &:first-child {
            padding-left: 10px;
          }
          &:last-child {
            padding-right: 10px;
          }
        }
      }
    }

    tbody {
      overflow-x: hidden;

      tr {
        td {
          font-size: 10px;
          font-weight: 400;
          padding: 2px;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
          height: 22px;

          &:first-child {
            padding-left: 10px;
          }
          &:last-child {
            padding-right: 10px;
          }
        }
      }
    }
  }
}
}

.mobileBackButton {
  position absolute;
  left: 12px;
  top: 10px;
  font-size: 26px;
  z-index: 1;
}

.mobileSellButtons {
  display: flex;
  justify-content: space-between;
  align-items: center;

  padding: 10px;

  background-color: transparent;

}

.mobileTabs {
  display: flex;
  flex-direction: column;
  position: relative;
  height: 100%;
  width: 100%;

  > :global(.lat-tabs-bar) {
    padding-left: 45px;
    margin-bottom: 0;
    background: #fff;

    :global(.lat-tabs-tab) {
      text-transform: uppercase;
      font-size: 16px;
      font-weight: 500;
      margin: 0;
      color: #949494;
      white-space: no-wrap;

      &:hover {
        color: black;
      }
    }
  }


  > :global(.lat-tabs-content) {
    height: 100% !important;

    > :global(.lat-tabs-tabpane) {
      display: flex;
      flex-direction: column;
      padding: 8px !important;

      &:global(.lat-tabs-tabpane-inactive) {
        padding: 8px !important;
      }
    }
  }

  :global(.lat-table-row td) {
    height: 40px !important;
  }


  :global(.lat-tabs-tab-active) {
    opacity: 1 !important;
  }


  :global(.chart-controls-bar) {
    display: none !important;
  }

  div.chartContainer {
    display: flex;
    flex-direction: column;
    flex: 1;
    height: 100% !important;

    .chartHeader {
      transform: none;
      padding: 6px 15px 0 15px;
      position: relative;
      top: 0;

      > * {
        height: 30px;
        margin-right: 10px;
        font-size: 14px;
        border-radius: 0;

      }
      :global(.ant-select-selection) {
        height: 30px;
        border-radius: 0;
        > * {
          line-height: 30px;
          font-size: 14px;
        }
      }
    }

    div.chartWrapper {
      //height: calc(100% - 52px) !important;
      //display: flex;
      //flex-direction: column;
      height: 100% !important;
      flex: 1;
    }
  }

  .centerPanel {
    flex: 1 1 auto;
    margin: 0;
    padding: 0;
    .resizer {
      display none;
    }

    .chartBlock {
      display: flex;
      flex-direction: column;
      margin: 0;
      padding: 0;
      flex: 1 0;
    }
  }

  .ordersContainer {
    height: auto !important;
    flex: 1 0 auto;

    :global(.lat-tabs-tab) {
      margin-right: 12px;

